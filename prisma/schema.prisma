generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          String  @id
  email       String  @unique
  password    String
  phone       String
  name        String
  city        String? @default("")
  description String  @default("")
  avatarUrl   String  @default("")
}

model Media {
  id       String @id @default(uuid())
  mediaId  String
  cdnUrl   String
  mimeType String
}
